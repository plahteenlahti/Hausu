{
  "expo": {
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "13171d92-1d7b-45fc-947f-b072484496a6"
      }
    },
    "scheme": "tamagui-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#fff",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "icon": "./assets/icon.png",
    "name": "amira",
    "slug": "amira",
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "ios": {
      "bundleIdentifier": "digital.fenrir.amira"
    },
    "android": {
      "package": "digital.fenrir.amira"
    }
  },
  "plugins": [
    [
      "expo-build-properties",
      {
        "android": {
          "kotlinVersion": "1.6.20",
          "compileSdkVersion": 33,
          "targetSdkVersion": 33,
          "minSdkVersion": "23",
          "buildToolsVersion": "33.0.0",  
          "packagingOptions": {
            "pickFirst": ["**/libc++_shared.so"]
          }
        }
      }
    ],
    [
      "expo-image-picker",
      {
        "photosPermission": "The app accesses your photos to let add images to properties."
      }
    ],
    "./plugins/watermelon.js"
  ]
}
