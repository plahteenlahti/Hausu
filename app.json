{
  "expo": {
    "scheme": "tamagui-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#fff",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "icon": "./assets/icon.png",
    "name": "amira",
    "slug": "amira",
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "ios": {
      "bundleIdentifier": "digital.fenrir.amira"
    }
  },
  "plugins": [
    [
      "expo-build-properties",
      {
        "android": {
          "kotlinVersion": "1.6.10",
          "compileSdkVersion": 33,
          "targetSdkVersion": 33,
          "packagingOptions": {
            "pickFirst": ["**/libc++_shared.so"]
          }
        }
      }
    ],
    [
      "expo-image-picker",
      {
        "photosPermission": "The app accesses your photos to let add images to properties."
      }
    ],
    "./plugins/watermelon.js"
  ]
}
